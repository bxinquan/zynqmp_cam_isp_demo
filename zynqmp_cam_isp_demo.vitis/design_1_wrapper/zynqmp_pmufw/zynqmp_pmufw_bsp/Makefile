BSP_SEQUENTIAL_MAKEFILES := psu_pmu_0/libsrc/avbuf_v2_5/src/Makefile psu_pmu_0/libsrc/dpdma_v1_4/src/Makefile psu_pmu_0/libsrc/dppsu_v1_6/src/Makefile psu_pmu_0/libsrc/mipi_rx_to_video_v1_0/src/Makefile psu_pmu_0/libsrc/xilsecure_v4_7/src/Makefile psu_pmu_0/libsrc/xilskey_v7_3/src/Makefile psu_pmu_0/libsrc/xil_isp_lite_v1_0/src/Makefile psu_pmu_0/libsrc/xil_vip_v1_0/src/Makefile
